# Ambiente de Produção
# COPIE ESTE ARQUIVO PARA .env E CONFIGURE AS VARIÁVEIS

NODE_ENV=production
PORT=3333

# PostgreSQL Docker (configurado automaticamente pelo deploy.sh)
DATABASE_URL="postgresql://minhas_financas_user:SENHA_DO_POSTGRES@localhost:5432/minhas_financas_db?schema=public"

# JWT - MUDE PARA UM SECRET FORTE!
JWT_SECRET="GERE_UM_SECRET_FORTE_AQUI"
JWT_EXPIRES_IN=7d

# CORS - IP do seu servidor
CORS_ORIGIN=http://SEU_IP

# Logs
LOG_LEVEL=info
